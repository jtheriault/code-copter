{
  "name": "code-copter",
  "version": "1.2.2",
  "description": "Integrate JavaScript static code analysis into test framework",
  "scripts": {
    "examples": "scripts/run-examples",
    "test": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover `which jasmine`",
    "posttest": "istanbul check-coverage",
    "watch:test": "nodemon -x npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jtheriault/code-copter.git"
  },
  "keywords": [
    "test",
    "jasmine",
    "jshint",
    "style guide"
  ],
  "author": "Joe Theriault <joseph@yankoismedia.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jtheriault/code-copter/issues"
  },
  "dependencies": {
    "jasmine": "~2.3.2",
    "jscs": "~2.2.1",
    "jshint": "~2.8.0",
    "walk": "~2.3.9"
  },
  "devDependencies": {
    "istanbul": "^0.3.22",
    "nodemon": "~1.7.1",
    "proxyquire": "^1.7.3"
  }
}
