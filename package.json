{
  "name": "code-copter",
  "version": "1.5.1",
  "description": "Integrate JavaScript static code analysis into test framework",
  "scripts": {
    "clean": "rm -r coverage/ node_modules",
    "examples": "scripts/run-examples",
    "ls:todo": "egrep -nr '\\W+TODO\\W+.*$' src/",
    "test": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover `which jasmine`",
    "posttest": "istanbul check-coverage",
    "watch:test": "nodemon -i coverage -x npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jtheriault/code-copter.git"
  },
  "keywords": [
    "test",
    "jasmine",
    "jshint",
    "style guide"
  ],
  "author": "Joe Theriault <joseph@yankoismedia.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jtheriault/code-copter/issues"
  },
  "dependencies": {
    "code-copter-reporter-jasmine": "^1.0.0",
    "extend": "^3.0.0",
    "jscs": "~2.2.1",
    "jshint": "~2.8.0",
    "parent-require": "^1.0.0",
    "walk": "~2.3.9"
  },
  "devDependencies": {
    "istanbul": "^0.3.22",
    "jasmine": "~2.3.2",
    "nodemon": "~1.7.1",
    "proxyquire": "^1.7.3"
  }
}
